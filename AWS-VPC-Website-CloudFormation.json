      "PublicWebsite": {
          "Type": "AWS::EC2::Instance",
          "Properties": {
              "InstanceType": {
                  "Ref": "InstanceType"
              },
              "ImageId": {
                  "Fn::FindInMap": [
                      "AWSRegionArch2AMI",
                      {
                          "Ref": "AWS::Region"
                      },
                      {
                          "Fn::FindInMap": [
                              "AWSInstanceType2Arch",
                              {
                                  "Ref": "InstanceType"
                              },
                              "Arch"
                          ]
                      }
                  ]
              },
              "KeyName": {
                  "Ref": "KeyName"
              },
              "NetworkInterfaces": [
                  {
                      "GroupSet": [
                          {
                              "Ref": "PublicWebsiteSG"
                          }
                      ],
                      "AssociatePublicIpAddress": "true",
                      "DeviceIndex": "0",
                      "DeleteOnTermination": "true",
                      "SubnetId": {
                          "Ref": "PublicSubnet"
                      }
                  }
              ],
              "UserData": {
                  "Fn::Base64": {
                      "Fn::Join": [
                          "",
                          [
                              "#!/bin/bash -xe\n",
                              "mkdir -p /var/www\n",
                              "chmod 0755 /var/www\n",
                              "while [ ! -e /dev/xvdj ]; do echo waiting for /dev/xvdj to attach; sleep 10; done\n",
                              "mkfs -t ext4 /dev/xvdj\n",
                              "mount /dev/xvdj /var/www\n",
                              "echo \"/dev/xvdj /var/www ext4 defaults,nofail 0 2\" >> /etc/fstab\n",
                              "#yum install -y aws-cfn-bootstrap\n",
                              "# Install the files and packages from the metadata\n",
                              "/opt/aws/bin/cfn-init -v ",
                              "         --stack ",
                              {
                                  "Ref": "AWS::StackName"
                              },
                              "         --resource PublicWebsite ",
                              "         --configsets All ",
                              "         --region ",
                              {
                                  "Ref": "AWS::Region"
                              },
                              "\n",
                              "# Signal the status from cfn-init\n",
                              "/opt/aws/bin/cfn-signal -e $? ",
                              "         --stack ",
                              {
                                  "Ref": "AWS::StackName"
                              },
                              "         --resource PublicWebsite ",
                              "         --region ",
                              {
                                  "Ref": "AWS::Region"
                              },
                              "\n"
                          ]
                      ]
                  }
              }
          },
          "Metadata": {
              "AWS::CloudFormation::Designer": {
                  "id": "b22a4f40-7f9d-4f15-9a50-e74a47b404b1"
              },
              "AWS::CloudFormation::Init": {
                  "configSets": {
                      "All": [
                          "InstallWebServer",
                          "LoadDocs",
                          "LaunchWebServer"
                      ]
                  },
                  "InstallWebServer": {
                      "packages": {
                          "yum": {
                              "httpd": []
                          }
                      }
                  },
                  "LoadDocs": {
                      "sources": {
                          "/tmp": "https://github.com/freemansoft/AWS-website-demo/archive/master.zip"
                      },
                      "commands": {
                          "01_set_dir_mod": {
                              "command": "find html -type d -exec chmod 755 {} +",
                              "cwd": "/var/www"
                          },
                          "02_copy_docs": {
                              "command": "cp -r * /var/www/html",
                              "cwd": "/tmp/AWS-website-demo-master/html"
                          },
                          "03_chmod_docs": {
                              "command": "find html -type f -exec chmod 644 {} +",
                              "cwd": "/var/www"
                          }
                      }
                  },
                  "LaunchWebServer": {
                      "services": {
                          "sysvinit": {
                              "httpd": {
                                  "enabled": "true",
                                  "ensureRunning": "true"
                              }
                          }
                      }
                  }
              }
          }
      },
